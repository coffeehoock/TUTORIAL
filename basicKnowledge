  $headCount = 0;
  $flipCount = 0;

  while ($headCount < 3) {
    $flip = rand(0,1);
    $flipCount ++;
    if ($flip){
      $headCount ++;
      echo "<div class=\"coin\">H</div>";
    }
    else {
      $headCount = 0;
      echo "<div class=\"coin\">T</div>";
    }
  }
  echo "<p>случайное число  равно {$flipCount}</p>";



    $flipCount = 0;

      do {
        $flip = rand(0,1);
        $flipCount ++;
        if ($flip){
            echo "<div class=\"coin\">H</div>";
        }
        else {
            echo "<div class=\"coin\">T</div>";
        }
    } while ($flip);
    $verb = "were";
    $last = "flips";
    if ($flipCount == 1) {
        $verb = "was";
        $last = "flip";
    }
    echo "<p>There {$verb} {$flipCount} {$last}!</p>";