{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "chai",
    "jquery",
    "underscore"
  ],
  "loadEagerly": [
    "path/to/your/js/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**",
    "path/to/your/js/**/*.js"
  ],
  "plugins": {
    "modules": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "angular": {},
    "requirejs": {
      "baseURL": "",
      "paths": "",
      "override": ""
    },
    "commonjs": {}
  }
}